{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "next/core-web-vitals",
    "google",
    "prettier",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "require-jsdoc": 0,
    "jsx-a11y/alt-text": 1
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
